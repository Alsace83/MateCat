.menucolor li a:hover, .d:hover{background-color:#f7f7f7}
.arrow-mcolor{display:block;width:20px;height:20px;background:url(../img/arrow-menucolor.png) 0 0 no-repeat;position:absolute;margin:-8px 0 0 120px}
.a{	background-position: 119px -22px !important;}
.d{background-position: 119px -79px !important;}
.r{background-position: 119px -51px !important;}









/*joblist*/
 .joblist {	display:none;	background:#f3f3f3;	position:absolute;width:auto;min-width:200px;	z-index:100;	border:1px solid #000;	text-align:left;	padding:0;	top: 35px; 	text-shadow:none;	margin:5px 0 0 0px;*margin-right: -1px;	color:#000;	font-size:14px;	-moz-box-shadow: 0 0 5px #888;-webkit-box-shadow: 0 0 5px #000;box-shadow: 0 0 5px #000;}
 .joblist .arrow-notification{margin:-9px 0 0 12px}
.joblist ul li{padding:0 !important }
.joblist ul li a{padding:15px }



/*notifications*/
.h-notification{background: url(../img/header-bg.jpg) 0 0 repeat-x;height:20px;color:#fff;font-weight:bold;text-shadow:none !important;padding:4px 0 0 10px;text-shadow: #333 0px 1px 2px;font-size:14px;}
#notification ul, .joblist ul{margin:0px 0 0 0;overflow-y:auto;max-height:200px}
#notification ul li, .joblist ul li{margin:0;padding:15px;border-bottom:1px solid #000;-webkit-transition: all  100ms ease-in; -moz-transition: all  100ms ease-in; }
#notification ul li a, .joblist ul li a{display:block;text-decoration:none;color:#000;}

#notification ul li:hover, .joblist ul li:hover{background:#fff}
#notification {	display:none;	background:#f3f3f3;	position:absolute;	width:270px;	z-index:100;	border:1px solid #000;	text-align:left;	padding:0;	top: 35px; 	text-shadow:none;	margin:5px 0 0 0px;right:0px;	*margin-right: -1px;	color:#000;	font-size:14px;	-moz-box-shadow: 0 0 5px #888;-webkit-box-shadow: 0 0 5px #000;box-shadow: 0 0 5px #000;}



.arrow-notification{display:block;width:25px;height:25px;background:url(../img/arrow-notification.png) 0 0 no-repeat;position:absolute;margin:-9px 0 0 252px}



.prova{;margin:180px auto 60px auto;}
.pulsante{background:#000 !important}




/*comment*/

.comment-area{position:absolute;top:3px;z-index:9999;background:#fff;-moz-box-shadow: 0 1px 3px #ccc;-webkit-box-shadow: 0 1px 3px #ccc;
margin:-7px 0 0 25px;border:1px solid #36698e;-moz-border-radius: 2px;border-radius: 2px;}



.openarea{top:-2px;}
.openarea .corner{margin:21px 0 0 -38px; background:url(../img/cornerbig.png) 0 0 no-repeat;}
.c-close{background:#fff;opacity:0.8;border:1px solid #36698e; -moz-border-radius: 2px;border-radius: 2px;color:#333;text-shadow:none;float:left;  overflow:hidden;-webkit-transition: all  100ms ease-in; -moz-transition: all  100ms ease-in;width:18px !important;text-align:left;margin:5px 0 0 10px;-moz-box-shadow: 0 1px 3px #ccc;-webkit-box-shadow: 0 1px 3px #ccc;}




.c-close:hover{opacity:1;-moz-box-shadow: 0 1px 3px #666;-webkit-box-shadow: 0 1px 3px #666;margin-top:-1px !important;}
.c-close .x-com{display:none;}

.comment-area ul{background:#fff;padding:0px 0px 0 0px;text-align:left;font-size:12px;-moz-border-radius: 2px;border-radius: 2px;line-height:16px;float:left;width:230px;}

.comment-area li{padding:5px 0 5px 0;margin:0;border-bottom:1px solid #36698e;width:230px;}
.corner{background:url(../img/comment-corner.png) 0 0 no-repeat;display:block;width:25px;height:20px;position:absolute;margin:2px 0 0 -24px;padding:10px 30px;cursor:pointer}
.date{color:#999;float:right}
.comment-area ul p{margin:0;padding:0 10px}
.spancomm {background:url(../img/bg-cmt.png) 0 0 repeat-x;padding:2px 5px 2px 0; }
.spancomm .lft, .spancomm .rgt{background:url(../img/cm-left.png) 0 0 no-repeat;display:block;width:10px;height:25px;float:left;margin:-3px 0 0 0}
.spancomm .rgt{background:url(../img/cm-right.png) 0 0 no-repeat;}
.con-input{float:left;width:77%;margin:10px 0 30px 10px;border:1px solid #727272;-moz-box-shadow: inset 0 1px 2px #888;
-webkit-box-shadow: inset 0 1px 2px #888;-moz-border-radius: 2px;border-radius: 2px;box-shadow: inner 0 1px 2px #888;padding:5px 5px 6px 5px;}
.comment-area .h-notification{text-align:left;margin:0;padding:4px 0 2px 10px;width:220px;}
.comment-area textarea{background:#fff;width:188px;margin:10px 0 0 10px;border:1px solid #36698e}
.x-com{background:url(../img/x.jpg) center center no-repeat;width:25px;display:inline;padding:0;margin:0;margin-top:-22px;position:absolute;right:0;border-left:1px solid #000;}



/*stats*/


.stats{display:none;width:465px; height:195px; background-color:#FFF; z-index:99999999; position:fixed; bottom:33px; right:35px;-moz-border-radius: 2px;  -webkit-border-radius: 2px; border:1px solid #000;	text-align:left;	padding:0;	text-shadow:none;	font-size:14px;	-moz-box-shadow: 0 0 5px #888;-webkit-box-shadow: 0 0 5px #000;box-shadow: 0 0 5px #000;color:#000;}
.stats ul{margin:0;}

.stats .first{width:220px}
.stats li{border-left:none;border-bottom:1px solid #666;width:35px}
.h-stats{height:30px;background: url(../img/header-bg.jpg) 0 0 repeat-x;color:#fff;text-shadow: #333 0px 1px 2px;}
.stats .arrow-notification{margin:195px 0 0 432px;background:url(../img/arrow-stats.png) 0 0 no-repeat;}
